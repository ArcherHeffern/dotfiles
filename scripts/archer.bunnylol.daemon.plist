<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.bunnylol.serve</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/bunnylol</string>
        <string>serve</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/bunnylol.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/bunnylol.err</string>
</dict>
</plist>
